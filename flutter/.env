# Usuário e senha padrão (login automático/testes)
FLUTTER_DEFAULT_USER=admin
FLUTTER_DEFAULT_PASSWORD=supersecreta123

# URL base do backend (ajuste se usar outra porta, endereço ou deploy em nuvem)
API_BASE_URL=http://localhost:8000

# Endpoint de autenticação (separado para maior flexibilidade)
API_LOGIN_ENDPOINT=/login

# Chave opcional para autenticação (JWT, se usado pelo frontend)
API_JWT_KEY=

# Ambiente do app (dev, staging, prod)
FLUTTER_ENV=dev

# Nome do app para exibição ou logs
APP_DISPLAY_NAME=OneAPIIRS

# Exemplo de timeout para requisições HTTP (em segundos)
HTTP_TIMEOUT=20

# (Opcional) Chave de analytics ou tracking se for integrar futuramente
ANALYTICS_KEY=

# (Opcional) Feature flag para mostrar funcionalidades beta
ENABLE_BETA_FEATURES=false
